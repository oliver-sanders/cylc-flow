[scheduling]
    [[graph]]
        R1 = t1 => t2 & t3
[runtime]
    [[t1]]
        script = """
            cylc client \
                "${CYLC_SUITE_NAME}" \
                get_graph_raw \
                >"${CYLC_SUITE_RUN_DIR}/ctb-get-graph-raw.out" \
                <<__HERE__
            {
                "start_point_string": "1",
                "stop_point_string": "1",
                "group_nodes": "T",
                "ungroup_nodes": null,
                "ungroup_recursive": false,
                "group_all": false,
                "ungroup_all": false
            }
            __HERE__
        """
    [[T]]
        script = true
    [[t2, t3]]
        inherit = T
