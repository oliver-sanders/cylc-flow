[scheduler]
    [[events]]
        inactivity = PT1M
        timeout = PT1M
        abort on inactivity = true
        abort on timeout = true
[task parameters]
    i = 1..5
[scheduling]
    [[graph]]
        R1 = foo<i>
[runtime]
    [[foo<i>]]
        script = true
        platform = $(my-host-select)
        [[[job]]]
            submission retry delays = PT10S
