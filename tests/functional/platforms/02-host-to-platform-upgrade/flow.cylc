#!jinja2

[scheduler]
    UTC mode = True

[scheduling]
    [[graph]]
        R1 = """
            no_settings & upgradeable_cylc7_settings => fin
        """

[runtime]
    [[root]]
        script = true

    [[no_settings, fin]]

    [[upgradeable_cylc7_settings]]
        script = test {{CYLC_TEST_HOST_FQDN}} == "$(hostname -f)"
        [[[remote]]]
            host = {{CYLC_TEST_HOST}}
